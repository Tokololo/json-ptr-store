{
    "name": "@tokololo/json-ptr-store",
    "version": "1.0.11",
    "description": "A json pointer and rxjs enabled reactive store",
    "main": "./dist/cjs/index.js",
    "module": "./dist/mjs/index.js",
    "types": "./dist/mjs/index.d.ts",
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "exports": {
        ".": {
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "scripts": {
        "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
        "build:esm": "tsc --module esnext --outDir dist/mjs",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "prepare": "npm run build",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tokololo/json-ptr-store.git"
    },
    "keywords": [
        "typescript",
        "json-pointer",
        "json-ptr",
        "rxjs",
        "reactive",
        "store",
        "state-management"
    ],
    "author": "WillowDigit",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Tokololo/json-ptr-store/issues"
    },
    "homepage": "https://github.com/Tokololo/json-ptr-store#readme",
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "@types/json-pointer": "^1.0.34",
        "@types/lodash": "^4.14.202",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "clean-deep": "^3.4.0",
        "json-pointer": "^0.6.2",
        "lodash": "^4.17.21",
        "rxjs": "^7.8.1"
    }
}